{
    "transform": {
      "^.+\\.jsx?$": "babel-jest", 
      "^.+\\.tsx?$": "ts-jest"
      
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "collectCoverage":true,
    "coverageReporters":["text", "lcov", "clover"]
  }